<tests xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="urn:magento:mftf:Test/etc/testSchema.xsd">
    <test name="OpenStorefrontDemoPageTest">
        <annotations>
            <features value="Docker Test"/>
            <title value="Open test Docker page"/>
            <severity value="CRITICAL"/>
        </annotations>

        <amOnPage stepKey="amOnPage"  url="{{StorefrontDemoPage.url}}"/>
        <see stepKey="verifyText" userInput="Welcome to Cloud Docker MFTF Demo."/>
    </test>
</tests>
